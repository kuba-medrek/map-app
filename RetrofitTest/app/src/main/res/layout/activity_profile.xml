<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ad.retrofittest.Profile.Profile">

    <TextView
        android:id="@+id/loginTvIdProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="145dp"
        android:text="login" />

    <TextView
        android:id="@+id/nameTvIdProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/loginTvIdProfile"
        android:layout_alignStart="@+id/loginTvIdProfile"
        android:layout_below="@+id/loginTvIdProfile"
        android:layout_marginTop="26dp"
        android:text="name" />

    <TextView
        android:id="@+id/lastNameTvIdProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/nameTvIdProfile"
        android:layout_alignStart="@+id/nameTvIdProfile"
        android:layout_below="@+id/nameTvIdProfile"
        android:layout_marginTop="21dp"
        android:text="lastname" />

    <TextView
        android:id="@+id/birthdayTvIdProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lastNameTvIdProfile"
        android:layout_alignStart="@+id/lastNameTvIdProfile"
        android:layout_below="@+id/lastNameTvIdProfile"
        android:layout_marginTop="25dp"
        android:text="birthday" />

    <TextView
        android:id="@+id/descriptonTvIdProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/birthdayTvIdProfile"
        android:layout_alignStart="@+id/birthdayTvIdProfile"
        android:layout_below="@+id/birthdayTvIdProfile"
        android:layout_marginTop="34dp"
        android:text="description" />
</RelativeLayout>
